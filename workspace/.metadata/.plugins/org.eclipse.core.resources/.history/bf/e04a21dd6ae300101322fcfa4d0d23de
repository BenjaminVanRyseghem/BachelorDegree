import java.util.*;


public class FileFIFO<E> {
	List<E> internalList;
	int indiceTete;
	int longueur;
	
	public E addElement(E elt){
		E result;
		result = internalList.set(indiceTete,elt);
		this.incrementIndiceTete();
		return result;
	}
	
	public void incrementIndiceTete(){
		this.indiceTete = this.indiceTete + 1 % this.longueur;
	}
}
