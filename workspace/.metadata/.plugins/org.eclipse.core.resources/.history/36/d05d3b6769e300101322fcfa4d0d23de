import java.util.*;


public class FileFIFO<E> {
	List<E> list;
	int indiceTete;
	int largeur;
	
	public E addElement(E elt){
		E result;
		result = list.set(indiceTete,elt);
		this.incrementIndiceTete();
		
		return result;
	}
	
	public void incrementIndiceTete(){
		this.indiceTete ++;
		if (this.indiceTete > this.largeur){
			indiceTete = 1;
		}
	}
}
